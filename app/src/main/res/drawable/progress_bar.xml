<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@android:id/background"
        android:height="5dp">
        <shape>
            <solid android:color="#5b5b5b"
                android:height="5dp" />
        </shape>
    </item>

    <item
        android:id="@android:id/secondaryProgress"
        android:height="5dp">
        <clip>
            <shape>
                <solid android:color="#60ffffff"
                    android:height="5dp" />
            </shape>
        </clip>
    </item>

    <item
        android:id="@android:id/progress"
        android:height="5dp">
        <clip>
            <shape>
                <solid android:color="#8df542"
                    android:height="5dp" />
            </shape>
        </clip>
    </item>
</layer-list>